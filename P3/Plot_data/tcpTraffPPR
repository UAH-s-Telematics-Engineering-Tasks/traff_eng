set xlabel 'Time (sec)'
set ylabel 'Rate (kbps)'
set style data lines
set yrange[0.000000:*]
set key bottom right
plot \
'-' using 1:2 t 'TCP,::ffff:10.1.0.1/2000->10.1.5.1/5001~1'

